<form id="facebookform" role="form" action="#" method="POST">
  <div class="form-group">
    <h1> Sign In With Facebook </h1>
  </div>

  <div class="form-group">
    <input class="form-control" type="text" name="email" id="userEmail" placeholder="someone12@gmail.com">
  </div>

  <div class="form-group">
    <input class="form-control" type="password" name="password" id="userPassword" >
  </div>

  <div id="submit">
    <button type="submit" class="btn btn-default">Log In</button>
  </div>

</form>


<%= link_to 'Back', root_path %>
